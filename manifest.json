{
  "manifest_version": 2,
  "name": "TrumpToFartHead",
  "description": "Always replace Donald Trump's name with FartHead.",
  "version": "1.0",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
    },
  "browser_action": {
    "default_icon": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
